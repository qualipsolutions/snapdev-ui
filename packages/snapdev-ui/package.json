{
  "name": "@snapdev-ui/core",
  "version": "0.0.0",
  "private": false,
  "author": "Snapdev-UI Team",
  "description": "React component library for faster and consistent web development",
  "keywords": [
    "react",
    "material-ui",
    "snapdev-ui",
    "material",
    "design"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qualipsolutions/snapdev-ui.git",
    "directory": "packages/snapdev-ui"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qualipsolutions/snapdev-ui/issues"
  },
  "homepage": "https://github.com/qualipsolutions/snapdev-ui",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn build:es && yarn build:umd && yarn build:copy-files && yarn build:types",
    "build:cjs": "node ../../scripts/build cjs",
    "build:esm": "node ../../scripts/build esm",
    "build:es": "node ../../scripts/build es",
    "build:umd": "cross-env BABEL_ENV=production-umd rollup -c scripts/rollup.config.js",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "build:types": "tsc -p tsconfig.build.json",
    "extract-error-codes": "cross-env MUI_EXTRACT_ERROR_CODES=true yarn build:esm",
    "prebuild": "rimraf build",
    "release": "yarn build && npm publish build --tag latest",
    "test": "cd ../../ && cross-env NODE_ENV=test mocha 'packages/material-ui/**/*.test.js' --exclude '**/node_modules/**'",
    "typescript": "tslint -p tsconfig.json \"{src,test}/**/*.{spec,d}.{ts,tsx}\" && tsc -p tsconfig.test.json && tsc -p tsconfig.build.json"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/react": "^16.8.6 || ^17.0.0",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@types/react-transition-group": "^4.2.0",
    "clsx": "^1.0.4",
    "hoist-non-react-statics": "^3.3.2",
    "popper.js": "1.16.1-lts",
    "prop-types": "^15.7.2",
    "react-is": "^16.8.0 || ^17.0.0",
    "react-transition-group": "^4.4.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
