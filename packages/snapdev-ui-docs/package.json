{
  "name": "@snapdev-ui/docs",
  "version": "0.1.1",
  "private": true,
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "build": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired build",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn run build",
    "test": "yarn run build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public -o ./docs && cp ./docs/index.html ./docs/404.html"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@snapdev-ui/core": "^0.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "cross-env": "~7.0.2",
    "react-app-rewired": "~2.1.6",
    "tsconfig-paths-webpack-plugin": "~3.2.0",
    "typescript": "^4.1.2",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/node-logger": "^6.1.17",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.17",
    "react-scripts": "4.0.2"
  }
}
